---
interface Props {
  class?: string;
  width?: number;
  height?: number;
}

const { class: className = '', width = 16, height = 16 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <polyline points="6 9 12 15 18 9"></polyline>
</svg>
